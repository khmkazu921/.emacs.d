from mpl_toolkits.mplot3d import Axes3D
import matplotlib.pyplot as plt
import numpy as np

N = 160
M = 96
voxels = np.zeros((M,M,M))
for line in open("out", "r"):
    x, y, z = [int(i) for i in line[:-1].split(' ')]
    if x>=N and y>=N and z>=N:
        voxels[x-N][y-N][z-N] = 1

#print(ma)

fig = plt.figure()
ax = fig.gca(projection='3d')
ax.voxels(voxels)

plt.show()
