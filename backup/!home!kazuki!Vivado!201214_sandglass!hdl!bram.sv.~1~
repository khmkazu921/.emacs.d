interface bram;
    parameter int DATA_WIDTH = 3072;
    parameter int ADDR_WIDTH = 12;
    
    wire                  ena;
    wire [ADDR_WIDTH-1:0] addra; // 12 depth:2560
    wire [DATA_WIDTH-1:0] dina;  // 3072
    wire                  wea;
    wire                  enb;
    wire [ADDR_WIDTH-1:0] addrb; // 12 depth:2560
    wire [DATA_WIDTH-1:0] doutb; // 3072

endinterface
