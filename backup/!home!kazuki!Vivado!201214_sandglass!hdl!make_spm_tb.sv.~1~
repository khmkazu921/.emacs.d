`include "axi3.sv";

module make_spm_tb ();
    
    reg clk;
    reg resetn;
    
    initial begin
        clk = 0;
        resetn = 0;
    end 

    always #5
        clk = ~clk;
    
    
    
    make_spm make_spm_0
      (
        .clk(clk),
        .resetn(resetn)
      );
endmodule
