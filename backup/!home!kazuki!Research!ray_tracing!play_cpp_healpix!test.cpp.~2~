#include <iostream>
#include <math.h>
#include <healpix_cxx/healpix_base.h>

#define N_SIDE (16)
#define N_ANG 12*N_SIDE*N_SIDE

using namespace std;

int main() {
        for(int ipix=0; ipix<N_ANG; ipix++){
                Healpix_Base healpix(log2(N_SIDE),NEST);
                cout << healpix.pix2vec(ipix);
        }
        return 0;
}
